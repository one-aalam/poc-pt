    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "body-parser": "1.14.1",
    "cookie-parser": "1.4.0",
    "css-loader": "0.23.0",
    "dot": "1.0.3",
    "dotjs": "0.2.0",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "0.9.1",
    "http-server": "0.8.5",
    "jquery": "2.1.4",
    "json-loader": "0.5.4",
    "lodash": "3.10.1",
    "marko": "2.7.30",
    "morgan": "1.6.1",
